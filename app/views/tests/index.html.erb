<div class="container">
  <span class="text-center">
    <h1><%= t('.header') %></h1>
  </span>


  <table class="table">
    <thead >
      <tr>
        <th class="sort-by-title pointer">
          <%= t('admin.tests.index.title') %>
          <%= octicon 'arrow-up', class: 'text-success hide' %>
          <%= octicon 'arrow-down', class: 'text-success hide' %>
        </th>
        <th><%= t('admin.tests.index.level') %></th>
        <th ><%= t('admin.tests.index.questions') %></th>
        <th><%= t('.timer') %></th>
        <th><%= t('admin.tests.index.actions') %></th>
      </tr>
     <%= render @tests %>
    </thead>
  </table>

</div>


